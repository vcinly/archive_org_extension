{
    "name": "Arichive",
    "description" : "Archive Web Page",
    "version": "0.0.1",
    "manifest_version": 2,
    "browser_action": {
      "default_icon": "logo_archive-sm.png"
    },
    "background": {
        "scripts": ["js/background.js"],
        "persistent": false
    },
    "permissions": ["contextMenus","tabs", "storage","notifications", "http://*/*", "https://*/*"]
  }